{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "angular/controller-as": 0,
    "angular/controller-as-vm": 0,
    "angular/document-service": 0,
    "angular/no-private-call": 0,
    "angular/on-watch": 0
  }
}
